        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp ESC   &kp F1   &kp F2   &kp F3    &kp F4    &kp F5   &kp F6 &kp F7    &kp F8    &kp F9   &kp F10   &kp F11    &kp F12   &kp INS       &kp DEL   &kp HOME
